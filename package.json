{
  "name": "react-large-virtualized-list",
  "version": "1.0.0-development",
  "description": "React component for efficiently rendering large lists (Only renders items visible in the current viewport)",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "jest --watch --no-cache",
    "test:coverage": "jest --coverage",
    "open:coverage": "open ./coverage/lcov-report/index.html",
    "travis-deploy-once": "travis-deploy-once --pro",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "babel-jest": "^27.2.0",
    "jest": "^27.2.0",
    "jest-dom": "^4.0.0",
    "nwb": "0.25.x",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-testing-library": "^8.0.1",
    "semantic-release": "^17.4.7",
    "travis-deploy-once": "^5.0.7"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bramgiessen/react-large-virtualized-list.git"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "keywords": [
    "react-component"
  ]
